{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "outDir": "./wwwroot/js",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  
  "exclude": [
    "node_modules"
  ]
}
